{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "extends": ["airbnb"],
  "plugins": [
    "react"
  ],
  "rules" : {
    "arrow-body-style": 0,
    "allowForLoopAfterthoughts": 0,
    "import/prefer-default-export": 0,
    "no-unused-expressions": 0,
    "curly": 0,
    "class-methods-use-this": 0,
    "arrow-parens": 0,
    "react/prefer-stateless-function": 0,
    "react/jsx-closing-bracket-location": 0,
    "react/jsx-indent": 0,
    'react/forbid-prop-types': 0,
  }
}
